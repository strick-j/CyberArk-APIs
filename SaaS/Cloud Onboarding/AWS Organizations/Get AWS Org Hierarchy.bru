meta {
  name: Get AWS Org Hierarchy
  type: http
  seq: 1
}

get {
  url: https://{{identityTenantName}}-cloud_onboarding.cyberark.cloud/api/aws/organizations
  body: none
  auth: none
}

assert {
  res.body: isJson
  res.status: eq 200
}

script:pre-request {
  const platformTokenAuth = require('./tools/platformTokenAuth');
  
  // Perform authentication usings platformToken.js tools
  await platformTokenAuth.login();
}
